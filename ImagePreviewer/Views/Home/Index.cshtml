

@*@model ImagePreviewer.Models.ImageModel*@
@{
    ViewBag.Title = "Home Page";
}



<div class="row">
    <div class="col-md-3 col-sm-4 col-xs-6 thumb">

        <a class="compItem"  href='@Url.Action("Upload", "Image")'>
            <img src='@Url.Content("~/Content/Product_Images/add-button-inside-black-circle.jpg")'height="180" />
